{
  "name": "pneu-detector",
  "version": "0.0.1",
  "scripts": {
    "start": "polymer serve",
    "prebuild": "gulp optimize-images",
    "build": "polymer build --auto-base-path && gulp firebase",
    "build:static": "polymer build",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "serve": "firebase serve",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:prpl-server": "prpl-server --root server/build",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@firebase/app": "^0.4.1",
    "@firebase/auth": "^0.11.2",
    "@firebase/firestore": "^1.3.1",
    "@firebase/storage": "^0.2.16",
    "@material/card": "^2.1.1",
    "@polymer/app-layout": "^3.0.2",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/paper-spinner": "^3.0.2",
    "@polymer/polymer": "^3.2.0",
    "@vaadin/vaadin-upload": "^4.2.1",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "firebaseui": "^4.0.0",
    "lit-element": "^2.1.0",
    "lit-html": "^1.0.0",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-image-resize": "^0.13.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-webp": "^4.0.1",
    "imagemin-guetzli": "^2.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.9",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.5.0",
    "wct-mocha": "^1.0.0"
  }
}
